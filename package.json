{
  "name": "mev-bot",
  "version": "1.0.0",
  "description": "MEV arbitrage bot",
  "type": "commonjs",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "./scripts/start.sh",
    "start:ws": "node build/src/index.websocket.js",
    "start:mevshare": "node build/src/index.mevshare.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' hardhat test --config hardhat.config.cjs",
    "test:unit": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' hardhat test test/unit/*.test.ts --config hardhat.config.cjs",
    "test:integration": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' hardhat test test/integration/*.test.ts --config hardhat.config.cjs",
    "test:mevshare": "./scripts/test/mevshare.sh",
    "test:hybrid": "node scripts/test/hybrid.mjs",
    "build": "tsc",
    "build:ws": "tsc -p tsconfig.websocket.json",
    "build:all": "npm run build && npm run build:ws",
    "prepare": "npm run build",
    "lint": "npx eslint src",
    "compile": "tsc",
    "dev": "npm run build && npm run start",
    "dev:ws": "npm run build:ws && npm run start:ws",
    "monitor": "NODE_ENV=production node build/src/index.js",
    "monitor:ws": "NODE_ENV=production node build/src/index.websocket.js",
    "scanner": "ts-node -P tsconfig.scanner.json src/scanner/index.ts",
    "scanner:basic": "./scripts/scanner/basic.sh",
    "scanner:advanced:direct": "NODE_OPTIONS='--require ts-node/register' node -e \"require('./src/scanner/advanced-scanner.ts')\"",
    "scanner:advanced:ts": "npx tsx src/scanner/advanced-scanner.ts",
    "scanner:simple": "npx tsx -e \"console.log('\\nMEV Scanner Test'); const dotenv = require('dotenv'); dotenv.config(); console.log('Environment check complete. Use npm run scanner:advanced for full functionality.')\"",
    "scanner:advanced": "./scripts/scanner/advanced.sh",
    "build:scanner": "tsc -p tsconfig.scanner.json"
  },
  "dependencies": {
    "@ethersproject/providers": "5.7.2",
    "@ethersproject/wallet": "5.7.0",
    "@flashbots/ethers-provider-bundle": "0.6.2",
    "@flashbots/matchmaker-ts": "0.6.0",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.7.9",
    "colors": "^1.4.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.1",
    "ethers": "5.7.2",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "p-limit": "^5.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.11.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@aave/core-v3": "^1.19.3",
    "@babel/core": "^7.26.8",
    "@babel/plugin-syntax-flow": "^7.26.0",
    "@babel/plugin-transform-flow-strip-types": "^7.26.5",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^4.9.6",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.2.0",
    "@types/jasmine": "^3.6.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.202",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "@types/p-limit": "^2.1.0",
    "@types/sinon": "^17.0.3",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-jest": "^29.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.15.0",
    "ethereum-waffle": "^4.0.10",
    "hardhat": "^2.19.5",
    "hardhat-gas-reporter": "^1.0.8",
    "jasmine": "^3.6.3",
    "jest": "^29.7.0",
    "mocha": "^11.1.0",
    "nyc": "^15.1.0",
    "sinon": "^19.0.2",
    "solidity-coverage": "^0.8.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  }
}
